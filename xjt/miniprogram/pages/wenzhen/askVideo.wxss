/* pages/realVideo.wxss */
.head {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  margin-top: 100rpx;
}

.avatar image{
  border-radius: 50%;
  width: 150rpx;
  height: 150rpx;
}


.avatar text{
  color: rgb(70, 70, 70);
  font-weight: bold;
}

.video {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top: 150rpx;
}

.video image{
  width: 250rpx;
  height: 250rpx;
}